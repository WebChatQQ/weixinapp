<view class="z5 pop" >
  <view class="search-box border-box">
    <form bindsubmit="search">
      <view class="search-input-box">
        <input class="f-13rem f-f search-input" placeholder="请输入关键字" />
      </view>
      <view class="search-btn-box">
        <button size="mini" form-type="submit" type="primary">搜索</button>
      </view>
    </form>
  </view>
  <view style="width:100%;height:100%" catchtap="cancleSearch"></view>
</view>